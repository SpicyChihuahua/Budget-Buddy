<link rel="stylesheet" href="assets/css/register.css">
<br>
<br>
<p id="errorText"></p>

<div id="loginformSection">
    <p id="loginLabel">Register</p>
    <p id="info"> Enter a Username and Password you would like to use for your account</p>
    <form id="loginform" action="/processregister" method="POST">
        <p class="formText">Username:</p> 
        <input id="username" name="username" type="text" required/>
        <p class="formText">Password:</p> 
        <input id="password" name="password" type="password" required/><br>
        <input value = "create" id="submitButton" name="submitButton" type="submit"/>
    </form>
</div>